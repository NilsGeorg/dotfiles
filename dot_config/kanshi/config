
profile home {
	output eDP-1 disable
	output "LG Electronics LG HDR 4K 0x00060F37" mode 3840x2160@60.0 position 1600,0 scale 1.2
	output "LG Electronics LG Ultra HD 0x0000C49F" mode 3840x2160@30.0 position 4800,0 scale 1.2
}

profile solo {
	output eDP-1 enable mode 1920x1200 position 0,435 scale 1.2
}
